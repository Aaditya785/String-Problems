
public class Solution 
{

	public static boolean isPalindrome(String str) 
    {
		//Your code goes here
        int l = str.length()-1;
       for(int i=0;i<l;i++)
       {
           if(str.charAt(i)!=str.charAt(l))
           {
               return false;
           }
           l--;
       }
        return true;
	}

}
